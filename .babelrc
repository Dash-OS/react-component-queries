{
  "presets": [
    ["env", { "targets": { "node": true } }],
    "stage-3",
    "react"
  ],
  "plugins": [
    "transform-class-properties"
  ],
  "env": {
    "commonjs": {
      "presets": [
        "env",
        "stage-3",
        "react"
      ]
    },
    "umd": {
      "presets": [
        ["env", { "es2015": { "modules": false } }],
        "stage-3",
        "react"
      ]
    }
  }
}
